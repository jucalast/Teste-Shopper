datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Measure {
  id            String   @id @default(uuid())
  customerCode  String
  measureDatetime DateTime
  measureType   String
  measureValue  Int?
  imageUrl      String
  hasConfirmed  Boolean  @default(false)
  createdAt     DateTime @default(now())
}
